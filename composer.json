{
    "name": "marcel-mathias-nolte/contao-clothing-catalog",
    "description": "Clothing catalog extension for Contao Open Source CMS",
    "keywords": ["contao", "clothing", "catalog"],
    "type": "contao-module",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Marcel Mathias Nolte",
            "homepage": "https://github.com/marcel-mathias-nolte",
            "role":"Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "contao/core-bundle":"^4.4",
        "contao/faq-bundle": "^4.4",
        "contao/news-bundle": "^4.4",
        "contao/calendar-bundle": "^4.4",
        "contao-community-alliance/composer-plugin":"~2.4 || ~3.0",
        "codefog/contao-haste":"^4.17"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4": {
            "MarcelMathiasNolte\\ContaoClothingCatalogBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MarcelMathiasNolte\\ContaoClothingCatalogBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "contao-manager-plugin": "MarcelMathiasNolte\\ContaoClothingCatalogBundle\\ContaoManager\\Plugin"
    },
    "support": {
        "issues": "https://github.com/marcel-mathias-nolte/contao-clothing-catalog/issues",
        "source": "https://github.com/marcel-mathias-nolte/contao-clothing-catalog"
    },
    "config": {
        "allow-plugins": {
            "contao-components/installer": true,
            "contao-community-alliance/composer-plugin": true
        }
    }
}
